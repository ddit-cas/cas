<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- Root Context: defines shared resources visible to all other web components -->
	
	<bean class="com.cas.admin.notice.dao.impl.AdminNoticeDaoImpl"
		id="adminNoticeDao"
		p:client-ref="sqlMapClient"
	></bean>
	
	<bean class="com.cas.admin.notice.service.impl.AdminNoticeServiceImpl"
		id="adminNoticeService"
		p:adminNoticeDao-ref="adminNoticeDao"
	></bean>
	
</beans>
