<sqlMap namespace="Article">
	<typeAlias alias="str" type="java.lang.String" />
	<typeAlias alias="int" type="java.lang.Integer" />
	<typeAlias alias="article" type="com.cas.db.dto.ArticleVO"/>
	
	<insert id="insertArticle" parameterClass="article">
		INSERT INTO CONTENT VALUES (CONTENT_SEQ.NEXTVAL,#boardCode#,#contentTitle#,#contentContent#,SYSDATE,null,'test','n','y',null,null)																		
	</insert>
	
</sqlMap>